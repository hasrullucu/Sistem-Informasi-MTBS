            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="test text mantap"
                android:textColor="@color/white"
                android:fontFamily="@font/montserratbold"
                android:textSize="14sp"
                >

            </TextView>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="test text mantap"
                android:textColor="@color/white"
                android:fontFamily="@font/montserratbold"
                android:textSize="14sp"
                >

            </TextView>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/white"
                android:fontFamily="@font/montserratbold"
                android:text="test text mantap"
                android:textSize="14sp"
                >

            </TextView>

        </LinearLayout>




    </LinearLayout>




</LinearLayout>